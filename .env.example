# Discord bot token (from https://discord.com/developers/applications)
DISCORD_BOT_TOKEN=your-bot-token-here

# Discord application (client) ID
DISCORD_CLIENT_ID=your-client-id-here

# Optional: restrict command registration to a single guild for instant updates.
# Leave blank to register global commands (may take up to 1 hour to propagate).
DISCORD_GUILD_ID=

# Webhook URL used to post status / acknowledgment messages
STATUS_WEBHOOK_URL=https://discord.com/api/webhooks/your-webhook-url-here

# Directory where the command queue file is written
QUEUE_DIR=G:\My Drive\TDJ\Discord-Link

# Name of the queue file inside QUEUE_DIR
QUEUE_FILENAME=commands.txt

# How long (seconds) to wait after creating the queue file before the first
# "trading software didn't respond" warning.  Default: 120 (2 minutes)
INITIAL_WAIT_SECONDS=120

# How often (seconds) to repeat the warning while the file still exists.
# Default: 300 (5 minutes)
REPEAT_INTERVAL_SECONDS=300
